                        defer.resolve(_res);

                    }, function (err) {
                        defer.reject(err);
                    });

                }else {

                    if(keyArr){
                        if(keyArr.indexOf('data1') == -1 && keyArr.indexOf('data2') == -1 &&
                            keyArr.indexOf('data3') == -1 && keyArr.indexOf('data4') == -1 &&
