                                hideView();
                                if(onStart) onStart('match', match);
                                // if(typeof gameFuncCall != 'undefined') gameFuncCall('match', match);
                            })
                        }


                    }else if(res.returncode === CODE.SUCCESS_ERROR){
                        // todo : error exception
                        removeLoadingIcon();
                        UI_message(MESSAGES.ERROR_OCCURRED, 'OK');
                    }else {
