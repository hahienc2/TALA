                            addLoading();
                            getLBData(_idx, lbType, pageNo, function (res) {
                                hasNext = res.hasnext;
                                scroller.eventCallbackFinished();
                            });
                        }
                    }
                })

                getLBData(_idx, lbType, pageNo, function (res) {
                    hasNext = res.hasnext;
                    jq('.tab-content').children().each(function(idx){
                        if(_idx != idx){
                            jq(this).empty();
                        }
                    });
                });
            };
            _tab.open(0);
