                    }
                    setTimeout(function(){
                        jq('#msg-wrapper').remove();
                        $super.removeClass('view-on2');
                    }, aniDur - 100);
                }
            }else {
                // stack modal
                var $wrapper = jq(ev.currentTarget).closest('.msgs-wrapper');
                $wrapper.removeClass('on');
