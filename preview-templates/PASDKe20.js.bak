                var _name = arr[i].substr(delimeter);
                if(!_ext) _ext = _name.substr(_name.lastIndexOf('.')+1);
                var _temp = _name.substr(0, _name.lastIndexOf('.'));
                var _nameArr = _temp.split('-');
                var _i1 = parseInt(_nameArr[0])-1,  // 0...n
